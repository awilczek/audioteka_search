{
  "manifest_version": 3,
  "name": "Audioteka Better Search",
  "description": "Extension improving search in audioteka.pl",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "https://audioteka.com/pl/audiobooks/*"
      ]
    }
  ]
}