{
  "manifest_version": 3,
  "name": "Audioteka Better Search",
  "description": "Extension improving search in audioteka.pl",
  "version": "1.2",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "https://audioteka.com/pl/katalog/*",
        "https://audioteka.com/pl/cykl/*"
      ]
    }
  ]
}